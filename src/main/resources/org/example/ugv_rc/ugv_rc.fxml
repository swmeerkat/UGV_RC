<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="BOTTOM_CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
  fx:controller="org.example.ugv_rc.RcController">

  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>

  <Button text="Keyboard control"
    onAction="#enterKeyboardControl"
    onKeyPressed="#keyPressed"
    onKeyReleased="#keyReleased"/>
  <Button text="Do nothing"/>
</VBox>
